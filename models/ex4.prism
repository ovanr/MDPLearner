mdp

module main
    s : [0..7] init 0;
    
    [a] s=0 -> 0.8:(s'=2) + 0.2:(s'=1);
    [b] s=0 -> 1.0:(s'=3);
    [a] s=1 | s=4 -> 0.6:(s'=6) + 0.4:(s'=7);
    [b] s=1 | s=2 | s=4 | s=5 -> 1:(s'=7);
    [a] s=2 | s=5 -> 0.25:(s'=6) + 0.75:(s'=7);
    [a] s=3 -> 0.2:(s'=4) + 0.8:(s'=5);
    [b] s=3 -> 1:(s'=3);
    []  s=6 -> 1:(s'=6);
    []  s=7 -> 1:(s'=7);
endmodule
